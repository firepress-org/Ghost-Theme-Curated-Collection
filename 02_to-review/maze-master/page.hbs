{{!< default}}

{{#post}}
<div class="main-content">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="panel panel-default content-wrap">
                    <div class="panel-body">
                        <header class="content-header">
                            <h1 class="post-title">{{title}}</h1>
                        </header>
                        <section class="post-content">
                            <p>{{content}}</p>
                        </section>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

{{/post}}
